{
  "name": "reload",
  "displayName": "Reload Window",
  "description": "Add a reload window button to the status bar",
  "publisher": "tomjs",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "reload",
    "window",
    "statusbar",
    "重载",
    "重启"
  ],
  "engines": {
    "node": ">=14",
    "vscode": "^1.56.0"
  },
  "main": "dist/index.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjs/vscode-reload.git"
  },
  "bugs": {
    "url": "git+https://github.com/tomjs/vscode-reload/issues"
  },
  "homepage": "https://github.com/tomjs/vscode-reload/blob/main/README.md",
  "icon": "images/icon.png",
  "vsce": {
    "dependencies": false,
    "yarn": false
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint \"{src,scripts}/**/*.{js,cjs,ts}\" *.{js,cjs,ts} --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@tomjs/commitlint": "^2.1.2",
    "@tomjs/eslint": "^1.2.2",
    "@tomjs/prettier": "^1.1.1",
    "@tomjs/tsconfig": "^1.3.0",
    "@types/node": "^18.19.6",
    "@types/vscode": "^1.56.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.2",
    "tsup": "^8.0.1",
    "typescript": "~5.3.3"
  }
}
